class E {
    var map = {};

    operator ~ (str) {
        return this.map.get(str);
    }

    operator + (lst) {
        if (lst.instanceOf(List.type)) {
            this.map.put(lst.get(0), lst.get(1));
        }
    }
}

e = new E();

e + ["name", "sensei"];
