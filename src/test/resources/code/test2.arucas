fun schedule(milliseconds, function) {
    threaded(fun(ms, func) { 
        sleep(ms); 
        func(); 
    }, [milliseconds, function]);
}

schedule(1000, fun() {
    print("This will print second!");
});

print("This will print first!");

sleep(2000);

schedule(1000, fun() {
    string = input("input 2");
    print(string);
});

string = input("input 1");
print(string);