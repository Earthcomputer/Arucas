enum Direction {
    NORTH,
    SOUTH,
    EAST,
    WEST,
    UP,
    DOWN;

    embed String as s;

    Direction() {
        this.s = this.getName();
    }

    fun doThing(string) {
        print(this.s + string);
    }

    operator + (other) {
        return this.s + other;
    }
}