class Pos {
    var x;
    var y;
    var z;

    Pos(x, y, z) {
        this.x = x;
        this.y = y;
        this.z = z;
    }

    fun unpack() {
        return [this.x, this.y, this.z];
    }

    fun unpackReverse() {
        return [this.z, this.y, this.x];
    }
}

pos = new Pos(1, 2, 3);

z, y, x = pos.unpackReverse();

print(x);
print(y);
print(z);