enum Direction {
    NORTH,
    SOUTH,
    EAST,
    WEST,
    UP,
    DOWN;

    Direction() {

    }

    fun print() {
        print(this);
        print(this.getName().lowercase());
    }
}

foreach (direction : Direction.values()) {
    print(direction);
    direction.print();
}