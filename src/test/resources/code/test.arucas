class AccessibleList {
    var list;

    AccessibleList() {
        this.list = [];
    }

    AccessibleList(list) {
        if (list.instanceOf(List.type)) {
            this.list = list;
            return;
        }
        this.list = [];
    }

    operator [] (accessor) {
        return this.list.get(accessor);
    }
}

list = new AccessibleList([new AccessibleList([0]), 2, 3]);

print(list[0].list[0]);


