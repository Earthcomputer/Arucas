enum Colour {
    RED("Red"),
    GREEN("Green"),
    BLUE("Blue"),
    NONE;

    var prettyName;

    Colour(prettyName) {
        this.prettyName = prettyName;
    }

    Colour() {
        this.prettyName = "None";
    }

    fun toString() {
        return this.prettyName;
    }
}

new Colour();

